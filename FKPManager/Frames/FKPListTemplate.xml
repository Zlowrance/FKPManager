<Ui xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.blizzard.com/wow/ui/">
	<Script file="FKPListTemplate.lua" />
	<Frame name="FKPListTemplate" virtual="true">
		<Size x="283" y="80" />
		<Anchors>
			<Anchor point="CENTER" />
		</Anchors>
		<Backdrop bgFile="" edgeFile="">
			<Color r="1" g="0.753" b="0.753" />
			<BorderColor r="1" g="0.753" b="0.753" />
		</Backdrop>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture file="Interface\AddOns\FKPManager\assets\WhiteSquare.png" alphaMode="DISABLE" horizTile="true" vertTile="true">
					<Anchors>
						<Anchor point="TOPLEFT" />
						<Anchor point="BOTTOMRIGHT" />
					</Anchors>
					<Color r="1" g="0.753" b="0.753" />
				</Texture>
			</Layer>
			<Layer>
				<Texture name="$parentPortrait">
					<Size x="64" y="64" />
					<Anchors>
						<Anchor x="5" point="LEFT" />
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="OVERLAY">
				<FontString name="$parentName" inherits="GameFontNormal" text="FontString2" justifyH="LEFT">
					<Size x="195" y="20" />
					<Anchors>
						<Anchor x="75" y="-10" point="TOPLEFT" />
						<Anchor x="-10" y="-10" point="TOPRIGHT" />
					</Anchors>
				</FontString>
				<FontString name="$parentFKP" inherits="GameFontWhiteShadowLarge" text="FontString3" justifyH="LEFT">
					<Size x="176" y="20" />
					<Anchors>
						<Anchor x="75" point="LEFT" />
						<Anchor x="-10" point="RIGHT" />
					</Anchors>
				</FontString>
				<FontString name="$parentRoll" inherits="QuestDifficulty_Trivial" text="FontString3">
					<Size x="176" y="20" />
					<Anchors>
						<Anchor x="75" y="10" point="BOTTOMLEFT" />
						<Anchor x="-10" y="10" point="BOTTOMRIGHT" />
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<Button name="$parentRemoveButton" inherits="UIPanelCloseButton">
				<Size x="30" y="30" />
				<Anchors>
					<Anchor x="-10" point="RIGHT" />
				</Anchors>
				<NormalTexture file="Interface\AddOns\FKPManager\assets\CloseButton.png" />
				<PushedTexture file="Interface\AddOns\FKPManager\assets\CloseButton_Pushed.png" />
			</Button>
		</Frames>
	</Frame>
</Ui>